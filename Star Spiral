import turtle
from turtle import *
t = Turtle()

t.speed (20)

sidelength = 50
rotate = 144
def star(x,y):
    for i in range(5):
        t.forward(x)
        t.left(y)
star(sidelength, rotate)


def addSquares(iRange):
    length = 50

    for i in range(iRange):
        star(length, 144) 
        length +=5
        t.left(5)
       
            

addSquares(72)

turtle.done()
